<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqk.springcloudconsumer.mapper.TestUserMapper">

    <select id="getTest" resultType="java.lang.String">
        select age from test_user where id='1'
    </select>

    <update id="updateAge" >
        update test_user set age=#{age} where id='1'
    </update>


</mapper>